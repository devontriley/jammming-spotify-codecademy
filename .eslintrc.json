{
  "plugins": [
    "jest",
    "react"
  ],
  "env": {
    "jest/globals": true
  },
  "extends": [
    "next/core-web-vitals", 
    "next", 
    "prettier",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime"
  ],
  "rules": {
    "no-unused-vars": "off",
    "react/prop-types": "off"
  }
}
